@using Microsoft.AspNetCore.Identity
@using IptFinals.Areas.Identity.Data

@inject SignInManager<IptUser> SignInManager
@inject UserManager<IptUser> UserManager


@if (SignInManager.IsSignedIn(User))
{
    <a class="nav-item" >
        <a id="manage" class="nav-link text-white" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Hello @UserManager.GetUserName(User)!</a>
    </a>
}
else
{
   
}


